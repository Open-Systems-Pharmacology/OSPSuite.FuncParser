CC = g++

OBJECTS = Constant.o \
	Constants.o \
	DimensionInfo.o \
	DimensionParser.o \
	ElemFunction.o \
	ElemFunctions.o \
	FuncNode.o \
	FuncParser.o \
	FuncParserErrorData.o \
	Math.o \
	ParsedFunction.o \
	QuantityDimensionInfo.o \
	StringHelper.o


CFLAGS = -I../include -O3 -fPIC

LFLAGS = 

%.o:	%.cpp
	$(CC) $(CFLAGS) -c $*.cpp -o $*.o

funcparser:	$(OBJECTS)
	ar rc libfuncparser.a $(OBJECTS)


clean:
	rm -f *.o *~ core libfuncparser.a dep

dep:
	g++ -MM *.cpp >dep


